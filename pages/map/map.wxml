<!--pages/map/map.wxml-->
<view class="container">
  <map id="myMap" style="width:100%;height: 1200px;" latitude="{{latitude}}" longitude="{{longitude}}" scale="{{scale}}" markers='{{markers}}' bindmarkertap="onMarkerTap"bindregionchange="onRegionChange" />
  <detail markerInfo="{{markerInfo}}"></detail>
</view>
<tab-bar selected="{{selected}}" bind:tabChange="onTabChange"></tab-bar>